author = {Gronwald, F. and Hehl, F. W.},
eprint = {9602013},
file = {:home{\_}pers/peshkov/Documents/Mendeley Desktop/Gronwald, Hehl - 1996 - On the Gauge Aspects of Gravity.pdf:pdf},
month = {feb},
primaryClass = {gr-qc},
title = {{On the Gauge Aspects of Gravity}},
