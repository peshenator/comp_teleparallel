file = {:home{\_}pers/peshkov/Documents/Mendeley Desktop/Tenev, Horstemeyer - 2016 - The Mechanics of Spacetime - A Solid Mechanics Perspective on the Theory of General Relativity.pdf:pdf},
keywords = {2000,46,50,74l99,83d05,90,constitutive model,cosmic fabric,kd,mathematics subject classification,modified gravity,pacs 04,s,spacetime},
title = {{The Mechanics of Spacetime - A Solid Mechanics Perspective on the Theory of General Relativity}},
