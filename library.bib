@article{Lotte2018,
abstract = {OBJECTIVE {\&}{\#}13; Most current Electroencephalography (EEG)-based Brain-Computer Interfaces (BCIs) are based on machine learning algorithms. There is a large diversity of classifier types that are used in this field, as described in our 2007 review paper. Now, approximately 10 years after this review publication, many new algorithms have been developed and tested to classify EEG signals in BCIs. The time is therefore ripe for an updated review of EEG classification algorithms for BCIs.{\&}{\#}13; {\&}{\#}13; Approach:{\&}{\#}13; We surveyed the BCI and machine learning literature from 2007 to 2017 to identify the new classification approaches that have been investigated to design BCIs. We synthesize these studies in order to present such algorithms, to report how they were used for BCIs, what were the outcomes, and to identify their pros and cons.{\&}{\#}13; {\&}{\#}13; Main results:{\&}{\#}13; We found that the recently designed classification algorithms for EEG-based BCIs can be divided into four main categories: adaptive classifiers, matrix and tensor classifiers, transfer learning and deep learning, plus a few other miscellaneous classifiers.{\&}{\#}13; Among these, adaptive classifiers were demonstrated to be generally superior to static ones, even with unsupervised adaptation. Transfer learning can also prove useful although the benefits of transfer learning remain unpredictable. Riemannian geometry-based methods have reached state-of-the-art performances on multiple BCI problems and deserve to be explored more thoroughly, along with tensor-based methods.{\&}{\#}13; Shrinkage linear discriminant analysis and random forests also appear particularly useful for small training samples settings. On the other hand, deep learning methods have not yet shown convincing improvement over state-of-the-art BCI methods.{\&}{\#}13; {\&}{\#}13; Significance:{\&}{\#}13; This paper provides a comprehensive overview of the modern classification algorithms used in EEG-based BCIs, presents the principles of these methods and guidelines on when and how to use them. It also identifies a number of challenges to further advance EEG classification in BCI.},
author = {Lotte, Fabien and Bougrain, Laurent and Cichocki, Andrzej and Clerc, Maureen and Congedo, Marco and Rakotomamonjy, Alain and Yger, Florian},
doi = {10.1088/1741-2552/aab2f2},
file = {:C$\backslash$:/Users/Ilya/Google Drive/Mendeley Desktop/Lotte et al. - 2018 - A Review of Classification Algorithms for EEG-based Brain-Computer Interfaces A 10-year Update.pdf:pdf},
issn = {1741-2560},
journal = {Journal of Neural Engineering},
pages = {0--20},
pmid = {29488902},
publisher = {IOP Publishing},
title = {{A Review of Classification Algorithms for EEG-based Brain-Computer Interfaces: A 10-year Update}},
url = {http://iopscience.iop.org/article/10.1088/1741-2552/aab2f2},
year = {2018}
}
