isbn = {0022112008002},
issn = {2079-3197},
journal = {Computation},
keywords = {dns,hairpin vortices,navier-stokes equations,quadrant events,swirling-strength criterion for,turbulent channel flow,vortex detection},
number = {1},
pages = {13},
title = {{Direct Numerical Simulation of Turbulent Channel Flow on High-Performance GPU Computing System}},
