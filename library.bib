keywords = {3D printing,Densification,Mesoscopic simulation,Porosity,Selective laser melting,Thermodynamics},
number = {13},
pages = {1013--1022},
publisher = {Elsevier B.V. and Science China Press},
title = {{Selective laser melting 3D printing of Ni-based superalloy: understanding thermodynamic mechanisms}},
volume = {61},
year = {2016}
}
@article{Aposporidis2014,
abstract = {Copyright ? 2014, Kent State University.In the numerical solution of visco-plastic fluids, one of the hard problems is the effective detection of rigid or plug regions. These occur when the strain-rate tensor vanishes and consequently the equations for the fluid region become singular. In order to manage this lack of regularity, different approaches are possible. Regularization procedures replace the plug regions with high-viscosity fluid regions, featuring a regularization parameter ? {\textgreater} 0. In Aposporidis et al. [Comput. Methods Appl. Mech. Engrg., 200 (2011), pp. 2434-2446], an augmented formulation for Bingham fluids was introduced to improve the regularity properties of the problem. Results presented there show that the augmented formulation is more effective for numerical purposes and it works also in the non-regularized case (? = 0) without a significant degradation of the non-linear solver's performance. However, when solving high-dimensional Bingham problems, the augmented formulation leads to more challenging linear systems. In this paper we develop a strategy for preconditioning large non-regularized augmented Bingham systems. We use the regularized problem as a preconditioner for the non-regularized case. Then, we resort to a nonlinear geometric multilevel preconditioner to accelerate the convergence of the flexible Krylov linear solver for the regularized Bingham preconditioner. Results presented here demonstrate the effectiveness of the strategy also in realistic (non-academic) test cases.},
author = {Aposporidis, Alexis and Vassilevski, Panayot S. and Veneziani, Alessandro},
file = {:C$\backslash$:/Users/Ilya/Google Drive/Mendeley Desktop/Aposporidis, Vassilevski, Veneziani - 2014 - Multigrid preconditioning of the non-regularized augmented Bingham fluid problem.pdf:pdf},
issn = {10689613},
journal = {Electronic Transactions on Numerical Analysis},
keywords = {Bingham flow,Mixed finite elements,Multigrid,Multilevel flexible GMRES},
number = {January 2017},
pages = {42--61},
title = {{Multigrid preconditioning of the non-regularized augmented Bingham fluid problem}},
volume = {41},
year = {2014}
}
