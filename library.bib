journal = {Rheologica Acta},
keywords = {Interface,Non-Fickean diffusion,Polymeric blend,Traveling waves},
number = {7},
pages = {807--820},
title = {{Flow-diffusion interface interaction in blends of immiscible polymers}},
volume = {47},
