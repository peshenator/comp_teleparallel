@article{Frischmuth2002,
abstract = {Several technological situations at moderate and high temperatures as well as many physical experiments at low temperatures show the necessity of taking into account the wave structure of heat transport. Motivated by experimental evidence a thermo-mechanical framework for a deformable heat-conducting continuum has been developed. In this framework, a history effect is present, it is introduced via a proportionality law between the heat flux vector and the gradient of a scalar thermal variable. The theory leads to a modified model of thermoelasticity with an extra thermal stress effect and wave-type heat conduction. The model is governed by a system of quasi-linear hyperbolic equations. All essential material functions are examined, and the impact of their nonlinearity on the solution to initial-boundary value problems is studied. A numerical scheme is developed to solve initial-boundary value problems on finite domains by a modification of Lax-Wendroff's scheme. It turns out that the previously studied case of NaF crystals considered as rigid heat conductors was quantitatively quite acceptable: while it does not show any elastic waves, it still gives correct speeds and amplitudes of the pure thermal waves. However, this feature of upward compatibility of theories is lost for materials with higher thermal expansion coefficients. Hence, for new applications, it is of the essence to develop numerical methods for the fully coupled theory in 2D and 3D cases. {\textcopyright} 2001 Published by Elsevier Science B.V.},
author = {Frischmuth, K. and Kosi{\'{n}}ski, W.},
doi = {10.1016/S0165-2125(01)00064-6},
file = {:home{\_}pers/peshkov/Documents/Mendeley Desktop/Frischmuth, Kosi{\'{n}}ski - 2002 - Thermomechanical coupled waves in a viscoplastic medium.pdf:pdf},
issn = {03732029},
journal = {Archives of Mechanics},
keywords = {Hyperbolic systems,Method of preparation,Nonlinear wave propagation,Visco-plastic materials},
number = {5-6},
pages = {439--457},
title = {{Thermomechanical coupled waves in a viscoplastic medium}},
volume = {54},
year = {2002}
}
@article{Frigaard2005,
abstract = {Viscosity regularisation methods are probably the most popular current method for computing visco-plastic fluid flows. They are however generally used in an ad hoc manner. Here we examine convergence of regularised solutions to those of the corresponding exact models, in both mathematical and physical senses. Mathematically, the aim is to give practical guidance as to the order of error that one might expect for different regularisations and for different types of flow. Our theoretical results are illustrated with a number of computed example flows showing the orders of error predicted. Physically, the question is whether or not the regularised solutions behave in the same way as the exact solutions, qualitatively as well as quantitatively. We show that there are flows for which regularisation methods will generate their maximum errors, e.g. lubrication-type flows. In this context, we also consider the effects of regularisation on problems of hydrodynamic stability. For broad classes of problems, stability characteristics of the flow are incorrectly predicted by the use of viscosity regularisation methods. {\textcopyright} 2005 Elsevier B.V. All rights reserved.},
author = {Frigaard, I. A. and Nouar, C.},
doi = {10.1016/j.jnnfm.2005.01.003},
file = {:home{\_}pers/peshkov/Documents/Mendeley Desktop/Frigaard, Nouar - 2005 - On the usage of viscosity regularisation methods for visco-plastic fluid flow computation.pdf:pdf},
issn = {03770257},
journal = {Journal of Non-Newtonian Fluid Mechanics},
keywords = {Computational solution,Error analysis,Hydrodynamic stability,Visco-plastic fluid,Viscosity regularisation},
number = {1},
pages = {1--26},
title = {{On the usage of viscosity regularisation methods for visco-plastic fluid flow computation}},
volume = {127},
