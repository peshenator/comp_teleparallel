year = {2016}
}
@article{Blunt2001,
abstract = {This report describes progress from the Imperial College Consortium on Pore-Scale Modelling. The first chapter is a review paper written by Martin Blunt as an invited submission to Current Opinion in Colloid {\&} Interface Science. It briefly describes developments in pore-scale modelling in the last three years, with an emphasis on progress since 2000. Around 100 papers on network modelling have been published since 1998. The review puts this research in context and helps identify the major outstanding research issues, which are being tackled under this project. Chapter 2 is a paper by Martin Blunt submitted to the Journal of Colloid and Interface Science that derives relationships between interfacial tensions and contact angles for multiphase systems. implications for understanding three- and four-phase flows in porous media. Mohammad Piri and Per Valvatne started their PhDs funded by the project in October 2000. Since then they have made enormous progress. At the sponsors meeting in February they will present their work, including results from a predictive, random network model. With this report, I will also send you Mohammad Piri's MSc thesis. This was completed last summer and describes research on the development of a three- phase mixed-wet bundle-of-tubes network model. This is an excellent introduction to his present studies. The third chapter is a paper by Matthew Jackson and Martin Blunt presented at the 21st Annual International Energy Agency Workshop, Edinburgh, UK, 19-22 September 2000. Matt is a post-doc, who has been working on the project since October 1999 and has found some important results related to the description of three-phase flow. Our pore-to-core-to-resevoir upscaling methodology is described in this chapter. Then results from coupling a three-phase network model with a one-dimensional simulator are presented. The fourth chapter, by Matthew Jackson and Martin Blunt, is a paper submitted for publication in Transport in Porous Media. It extends the work of chapter 3, providing a thorough analysis of the mathematical properties of the three-phase flow equations. Using a physically-based model it is shown that the flow equations can develop so- called elliptic regions where the solutions are unstable in the absence of capillary pressure. Incorporating capillary pressure is necessary to ensure converged solutions to the flow equations. The consequences of this result for modelling three-phase displacements are discussed. The other member of our team is Pascal Audigane, a post-doc who joined Imperial in July 2000. At our meeting in February he will present his work on a dual mesh simulation method, coupling core-scale simulation to the field-scale. This is a work of basic physical chemistry with important January 2001 2},
author = {Blunt, Martin and Jackson, Matthew and Audigane, Pascal and Piri, Mohammad and Valvatne, Per},
isbn = {1124201198560},
number = {January},
pages = {1--59},
title = {{Imperial College Consortium on Pore-Scale Modelling}},
year = {2001}
}
@article{Reula2018,
abstract = {We develop a complete description of the class of conformal relativistic dissipative fluids of divergence form, following the formalism carried out by Geroch, Lindblom and Pennisi. This type of theories is fully described in terms of evolution variables whose dynamics is governed by total divergence-type conservation laws. Specifically, we give a characterization of the whole family of conformal fluids in terms of a single master scalar function defined up to second order corrections in dissipative effects, which we explicitly find in general form. This allows us to identify the equilibrium states of the theory, as well as to derive constitutive relations and a Fourier-like law for the corresponding first-order theory heat flux. Finally, we show that among this class of theories-- and near equilibrium configurations-- there exist symmetric hyperbolic ones, implying that for them one can define well posed initial value problems.},
