isbn = {9789048134465},
issn = {1478-6435},
journal = {Philosophical Magazine},
keywords = {balance laws,conservation laws,defects in solids,dislocation theory,dislocations,gauge theory,mechanics,noether symmetries},
month = {apr},
number = {11},
pages = {1673--1699},
title = {{The gauge theory of dislocations: conservation and balance laws}},
url = {http://www.tandfonline.com/doi/abs/10.1080/14786430802255653},
