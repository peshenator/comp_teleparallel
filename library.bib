issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {acoustics,complex fluids,granular media},
pages = {553--568},
title = {{Compressibility regularizes the $\mu$(I)-rheology for dense granular flows}},
url = {https://arxiv.org/pdf/1609.01502.pdf},
volume = {830},
year = {2017}
}
