@article{Adami2013,
abstract = {The standard weakly-compressible SPH method suffers from particle clumping and void regions for high Reynolds number flows and when negative pressures occur in the flow. As a remedy, a new algorithm is proposed that combines the homogenization of the particle configuration by a background pressure while at the same time reduces artificial numerical dissipation. The transport or advection velocity of particles is modified and an effective stress term occurs in the momentum balance that accounts for the difference between advection velocity times particle density and actual particle momentum. The present formulation can be applied for internal flows where the density summation is applicable. A wide range of test cases demonstrates unprecedented accuracy and stability of the proposed modification even at previously infeasible conditions. ?? 2013 Elsevier Inc.},
author = {Adami, S. and Hu, X. Y. and Adams, N. A.},
doi = {10.1016/j.jcp.2013.01.043},
file = {:C$\backslash$:/Users/Ilya/Google Drive/Mendeley Desktop/Adami, Hu, Adams - 2013 - A transport-velocity formulation for smoothed particle hydrodynamics.pdf:pdf},
isbn = {0021-9991},
issn = {00219991},
journal = {Journal of Computational Physics},
keywords = {Background pressure,Internal flows,Weakly-compressible SPH},
pages = {292--307},
title = {{A transport-velocity formulation for smoothed particle hydrodynamics}},
volume = {241},
year = {2013}
