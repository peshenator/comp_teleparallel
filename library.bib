keywords = {backstress,bauschinger effects,defect energy,dislocation density,gauge,geometrically necessary dislocations,gradient plasticity,incompatibility tensor,incompatible dis-,indeterminacy of,inequality,infinitesimal conformal mappings,isotropy,kinematic hardening,lanczos scalar,plasticity,rate-independent models,riemann-christoffel tensor,theory of dislocations,tortions,variational},
number = {June},
title = {{A fourth order gauge-invariant gradient plasticity model for polycrystals based on Kr$\backslash$"oner's incompatibility tensor}},
