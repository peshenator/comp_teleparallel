file = {:C$\backslash$:/Users/Ilya/Google Drive/Mendeley Desktop/Goddard, Lee - 2017 - On the stability of the $\mu$(I) rheology for granular flow.pdf:pdf},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
month = {dec},
pages = {302--331},
title = {{On the stability of the $\mu$(I) rheology for granular flow}},
url = {https://arxiv.org/pdf/1710.08139.pdf https://www.cambridge.org/core/product/identifier/S0022112017006516/type/journal{\_}article},
volume = {833},
