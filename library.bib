file = {:C$\backslash$:/Users/Ilya/Google Drive/Mendeley Desktop/Carter, Quintana - 1972 - Foundations of General Relativistic High-Pressure Elasticity Theory.pdf:pdf},
issn = {1364-5021},
journal = {Proceedings of the Royal Society A: Mathematical, Physical and Engineering Sciences},
number = {1584},
pages = {57--83},
title = {{Foundations of General Relativistic High-Pressure Elasticity Theory}},
url = {http://rspa.royalsocietypublishing.org/cgi/doi/10.1098/rspa.1972.0164},
