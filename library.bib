issn = {0034-4885},
journal = {Reports on Progress in Physics},
keywords = {Boltzmann equation,gradient expansion,holography,kinetic coefficients,quantum chromodynam-ics,quark-gluon plasma,relativistic heavy-ion collisions,relativistic kinetic theory,relativistic viscous hydrodynamics,resurgence,strongly-interacting matter},
