issn = {0022-2488},
journal = {Journal of Mathematical Physics},
month = {jan},
number = {1},
pages = {012901},
publisher = {American Institute of Physics},
title = {{The Weitzenb{\"{o}}ck connection and time reparameterization in nonholonomic mechanics}},
url = {http://aip.scitation.org/doi/10.1063/1.3525798},
volume = {52},
year = {2011}
