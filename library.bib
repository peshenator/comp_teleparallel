@article{Yavari2010,
abstract = {In this paper we formulate a geometric theory of the mechanics of growing solids. Bulk growth is modeled by a material manifold with an evolving metric. Time dependence of metric represents the evolution of the stress-free (natural) configuration of the body in response to changes in mass density and "shape". We show that time dependency of material metric will affect the energy balance and the entropy production inequality; both the energy balance and the entropy production inequality have to be modified. We then obtain the governing equations covariantly by postulating invariance of energy balance under time-dependent spatial diffeomorphisms. We use the principle of maximum entropy production in deriving an evolution equation for the material metric. In the case of isotropic growth, we find those growth distributions that do not result in residual stresses. We then look at Lagrangian field theory of growing elastic solids. We will use the Lagrange-d'Alembert's principle with Rayleigh's dissipation functions to derive all the governing equations. We make an explicit connection between our geometric theory and the conventional multiplicative decomposition of deformation gradient {\$}\backslashmathbf{\{}F{\}}=\backslashmathbf{\{}F{\}}{\_}e\backslashmathbf{\{}F{\}}{\_}g{\$} into growth and elastic parts. We linearize the nonlinear theory and derive a linearized theory of growth mechanics. Finally, we obtain the stress-free growth distributions in the linearized theory.},
author = {Yavari, Arash},
doi = {10.1007/s00332-010-9073-y},
file = {:C$\backslash$:/Users/Ilya/Google Drive/Mendeley Desktop/Yavari - 2010 - A geometric theory of growth mechanics.pdf:pdf},
issn = {09388974},
journal = {Journal of Nonlinear Science},
keywords = {Geometric elasticity,Growth mechanics,Material manifold},
number = {6},
pages = {781--830},
title = {{A geometric theory of growth mechanics}},
volume = {20},
year = {2010}
}
@article{Ng2008,
abstract = {This paper is dedicated to the identification of constitutive parameters of the Mohr–Coulomb constitutive model from in situ geotechnical measurements. A pressuremeter curve and the horizontal displacements of a sheet pile wall retaining an excavation are successively used as measurements. Two kinds of optimization algorithms are used to minimize the error function, the first one based on a gradient method and the second one based on a genetic algorithm. The efficiency of each algorithm related to the error function topology is discussed. Finally, it is shown that the use of a genetic algorithm to identify the soil parameters seems particularly suitable when the topology of the error function is complex.},
archivePrefix = {arXiv},
arxivId = {10.1002/nag.2347},
