journal = {Nonlinearity},
keywords = {Dispersive Shallow Water Equations,Euler-Lagrange Equations,Hyperbolic Systems},
number = {7},
pages = {2718--2736},
publisher = {IOP Publishing},
title = {{A rapid numerical method for solving Serre-Green-Naghdi equations describing long free surface gravity waves}},
