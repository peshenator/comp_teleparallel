journal = {Annual Review of Heat Transfer},
number = {1},
pages = {241--316},
title = {{Thermophysical Phenomena in Metal Additive Manufacturing by Selective Laser Melting: Fundamentals, Modeling, Simulation and Experimentation}},
url = {http://arxiv.org/abs/1709.09510 http://www.dl.begellhouse.com/references/5756967540dd1b03,562e7b3835dec96e,0860d4f32b9f248d.html},
