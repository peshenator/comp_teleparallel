isbn = {1932-6203 (Electronic)$\backslash$r1932-6203 (Linking)},
issn = {13596454},
journal = {Acta Materialia},
month = {jan},
number = {12},
pages = {107--120},
pmid = {21179216},
publisher = {Elsevier Ltd},
title = {{Fluid and particle dynamics in laser powder bed fusion}},
url = {https://doi.org/10.1016/j.actamat.2017.09.051 https://linkinghub.elsevier.com/retrieve/pii/S1359645417308170},
