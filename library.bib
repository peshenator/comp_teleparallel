isbn = {0377-0257},
issn = {03770257},
journal = {Journal of Non-Newtonian Fluid Mechanics},
keywords = {Crystalline solids,GENERIC framework,Multi-mode model,Non-equilibrium thermodynamics,Plastic spin,Plastic strain rate,Representation theorem,Slip systems,Viscoplasticity},
number = {1-3},
