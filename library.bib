doi = {10.1038/nphys3675},
eprint = {1510.07868},
file = {:home{\_}pers/peshkov/Documents/Mendeley Desktop/Lemoult et al. - 2016 - Directed percolation phase transition to sustained turbulence in Couette flow.pdf:pdf},
isbn = {1745-2473},
