keywords = {Powder bed fusion,Simulation,Solidification,Temperature},
number = {June 2018},
pages = {278--292},
title = {{Modeling and simulation of thermal field and solidification in laser powder bed fusion of nickel alloy IN625}},
volume = {109},
year = {2019}
}
