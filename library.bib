eprint = {1207.2929},
file = {:C$\backslash$:/Users/Ilya/Google Drive/Mendeley Desktop/Marchetti et al. - 2013 - Hydrodynamics of soft active matter.pdf:pdf},
isbn = {0034-6861},
issn = {00346861},
journal = {Reviews of Modern Physics},
number = {3},
pages = {1143--1189},
title = {{Hydrodynamics of soft active matter}},
volume = {85},
year = {2013}
}
