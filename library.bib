abstract = {This paper is an attempt to introduce methods and concepts of the Riemann-Cartan geometry largely used in such physical theories as general relativity, gauge theories, solid dynamics, etc. to fluid dynamics in general and to studying and modeling turbulence in particular. Thus, we further generalize our unified first-order hyperbolic formulation of continuum fluid and solid mechanics in order to account for rotational degrees of freedom (spin) of the underlying microscopic dynamics. Such a microscopic dynamics can be associated with the dynamics of small-scale eddies in turbulent flows of viscous fluids or dynamics of dislocations continuously distributed in an elastoplastically deformed solid. In order to achieve this, we use the rotational degree of freedom of the main field of the theory, the distortion field, which characterizes the deformation and rotation of the material elements and can be viewed as anholonomic basis triad with the non-vanishing torsion tensor. The torsion tensor is then used to characterize distortion's spin and is treated as an independent field with its own time evolution equation. The resulting governing equations have essentially the structure of the non-linear Maxwell equations in a moving medium. The system is closed by providing an example of the total energy potential. The extended system describes not only dissipative dynamics (which rises the entropy) due to viscosity or plasticity effects but it also has dispersive features which are due to the non-dissipative energy exchange (which conserves the entropy) between micro and macro scales. Both the dissipative and dispersive processes are represented by relaxation-type algebraic source terms so that the overall system remains first-order hyperbolic.},
archivePrefix = {arXiv},
arxivId = {1810.03761},
author = {Peshkov, Ilya and Romenski, Evgeniy and Dumbser, Michael},
eprint = {1810.03761},
file = {:home{\_}pers/peshkov/Documents/Mendeley Desktop/Peshkov, Romenski, Dumbser - 2018 - Continuum mechanics with torsion.pdf:pdf},
month = {oct},
title = {{Continuum mechanics with torsion}},
url = {http://arxiv.org/abs/1810.03761},
year = {2018}
}
