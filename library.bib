issn = {0374-3535},
journal = {Journal of Elasticity},
keywords = {Complete band-gaps,Fitting of the elastic coefficients,Free micro-inertia,Gradient micro-inertia,Inverse approach,Metamaterials,Microstructure,Phononic crystals,Relaxed micromorphic model},
month = {jan},
