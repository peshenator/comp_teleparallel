@article{Andrade2019,
abstract = {We study the mechanical response under time-dependent sources of a simple class of holographic models that exhibit viscoelastic features. The ratio of viscosity over elastic modulus defines an intrinsic relaxation time scale -- the so-called Maxwell relaxation time {\$}\backslashtau{\_}M{\$} -- that should determine the relaxation time. We compute explicitly the actual relaxation time and find that it is not really set by the Maxwell time but rather by the least damped quasi-normal mode. At high temperatures {\$}\backslashtau{\_}M{\$} over-estimates the actual relaxation time, although not by much and moreover it still captures reasonably well the temperature behaviour. At sufficiently low temperature the situation is reversed: {\$}\backslashtau{\_}M{\$} underestimates the actual relaxation time, in some cases quite drastically. Moreover, when {\$}\backslashtau{\_}M{\$} under-estimates the real-time response exhibits an over-shoot phenomenon before relaxation. We comment on the {\$}T=0{\$} limit, where the relaxation is power-law because our models exhibit criticality.},
archivePrefix = {arXiv},
arxivId = {1903.02859},
author = {Andrade, Tomas and Baggioli, Matteo and Pujolas, Oriol},
eprint = {1903.02859},
file = {:C$\backslash$:/Users/Ilya/Google Drive/Mendeley Desktop/Andrade, Baggioli, Pujolas - 2019 - Viscoelastic Dynamics in Holography.pdf:pdf},
