isbn = {0370-1573},
issn = {03701573},
journal = {Physics Reports},
number = {1-2},
pages = {1--171},
primaryClass = {gr-qc},
title = {{Metric-affine gauge theory of gravity: field equations, Noether identities, world spinors, and breaking of dilation invariance}},
