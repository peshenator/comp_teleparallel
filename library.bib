@article{Rosswog2015,
abstract = {We explore measures to increase the accuracy of SPH methods with respect to commonly used standard techniques. Our main focus here is special-relativistic SPH, but all measures can straight forwardly be applied in the Newtonian case as well. The first improvement concerns the calculation of gradients. Here a scheme that requires the (analytical) inversion of a small matrix is explored. For regular particle distributions this scheme yields gradient estimates that are many orders of magnitude more accurate than the standard SPH gradient. We apply such gradients in fully conservative special-relativistic SPH formulations and find in a large number of benchmark tests that they substantially increase SPH's accuracy. As a second measure, we explore a large number of kernel functions. The most commonly used cubic spline SPH kernel performs rather poorly, the best overall results are obtained for a high-order Wendland kernel which allows for only very little sub-resolution particle motion (noise) and enforces a very regular particle distribution. As a third improvement, we generalize a recently suggested SPH volume element to the special-relativistic case and find, as intended, that they remove surface tension effects and enhance the treatment of fluid instabilities. Fourth, we design new ways to steer dissipation with accurate triggers on both shocks and velocity noise. All tests, Newtonian and special-relativistic, are performed with a new SPH code, called SPHINCS{\_}SR. The new SPH formulation that includes all these improvements yields excellent results even in cases where standard techniques (high, constant dissipation; standard volume element; kernel gradients and the cubic spline kernel) completely fail (e.g. Gresho-Chan vortex or weakly triggered instabilities).},
archivePrefix = {arXiv},
arxivId = {arXiv:1405.6034v1},
author = {Rosswog, S.},
doi = {10.1093/mnras/stv225},
eprint = {arXiv:1405.6034v1},
file = {:home{\_}pers/peshkov/Documents/Mendeley Desktop/Rosswog - 2015 - Boosting the accuracy of SPH techniques Newtonian and special-relativistic tests.pdf:pdf},
issn = {13652966},
journal = {Monthly Notices of the Royal Astronomical Society},
keywords = {Hydrodynamics,Instabilities,Methods: numerical,Relativistic processes,Shock waves},
number = {4},
pages = {3628--3664},
title = {{Boosting the accuracy of SPH techniques: Newtonian and special-relativistic tests}},
volume = {448},
year = {2015}
}
